{
    "bip": "{{ docker_provisioning_docker_net_gateway }}/{{ docker_provisioning_docker_net_prefix_length }}",
    "fixed-cidr": "{{ docker_provisioning_docker_net_address }}/25",
    "mtu": 1500,
    "default-gateway": "{{ docker_provisioning_docker_net_base }}.254",
    "default-address-pools": [
    {
      "base": "{{ docker_provisioning_docker_net_addr_pool_base_0 }}",
      "size": {{ docker_provisioning_docker_net_addr_pool_size_0 }}
    }],
    "storage-driver": "overlay2",
    "userns-remap": "{{ docker_provisioning_dockerino_user_name }}",
    "log-driver": "json-file",
    "log-opts": {
      "max-size": "10m",
      "max-file": "3"
    }
}
